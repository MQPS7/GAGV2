local ModePlayer, ModePlayer_SelectFuntion = Create_ModeButton(" ‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô üë§ ")

local ModeInterFace, ModeInterFace_SelectFuntion = Create_ModeButton(" ‡∏≠‡∏¥‡∏ô‡πÄ‡∏ï‡∏≠‡∏£‡πå‡πÄ‡∏ü‡∏ã üåê ")

